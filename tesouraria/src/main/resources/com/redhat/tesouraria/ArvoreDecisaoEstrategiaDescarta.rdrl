package com.redhat.tesouraria;

import java.lang.Number;

rule "ArvoreDecisaoEstrategiaDescarta"
	dialect "mvel"
	no-loop true
	ruleflow-group "descartar"
	when
		op : Operacao( ( mesa == "CAMBIO" || mesa == "FX" )&& ( gestor == "CAMBIO" || gestor == "HEDGE" ))
	then
		System.out.println("ARVORE DE DECISAO ESTRATEGIA GESTOR: DESCARTA");
		modify( op ) {
				setDecisaoEstrategia( "DESCARTA" )
		}
end
