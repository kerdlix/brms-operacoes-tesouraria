package com.redhat.tesouraria;

import java.lang.Number;

rule "ArvoreDecisaoProdutoDescarta"
	dialect "mvel"
	no-loop true
	ruleflow-group "descartar"
	when
		op : Operacao( ( mesa == "CAMBIO" || mesa == "FX" )&& operadorNaLista == true )
	then
		modify( op ) {
				setDecisaoProduto( "DESCARTA" )
		}
end
