package com.redhat.tesouraria;

import java.lang.Number;

rule "ArvoreDecisaoProdutoAnalise"
	dialect "mvel"
	no-loop true
	ruleflow-group "analisar"
	when
		op : Operacao( decisaoProduto == null )
	then
		System.out.println("ARVORE DE DECISAO ESTRATEGIA PRODUTO: RETORNA PARA ANALISE");
		modify( op ) {
				setDecisaoProduto( "RETORNA PARA ANALISE" )
		}
end
